{
  "name": "@multi-channel-reply-assistant/shared",
  "version": "1.0.0",
  "description": "Shared utilities and types for Multi-Channel Reply Assistant",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./types": {
      "types": "./types/index.d.ts",
      "default": "./types/index.js"
    },
    "./constants": {
      "types": "./constants/index.d.ts",
      "default": "./constants/index.js"
    },
    "./utils": {
      "types": "./utils/index.d.ts",
      "default": "./utils/index.js"
    }
  }
}